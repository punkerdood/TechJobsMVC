<div class="container body-content">

    <h1>@ViewBag.title</h1>

    @{
        <table class="job-listing">
            @foreach (var job in ViewBag.jobs)
            {
                <tr>
                    <td>
                        <pre>@job.ToString()</pre>
                    </td>
                </tr>
            }
        </table>
    }


</div>